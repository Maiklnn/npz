- var class_name = 'card-list'
section(class=`${class_name} container`)
    div(class=`${class_name}__main`)
        div(class=`${class_name}__main-left`) 1

        div(class=`${class_name}__main-right`)

            div(class=`${class_name}__main-right-list`)

                - var item_good = 'item-list'
                mixin goods(list)
                    each item in list
                        div(class=`${item_good}`)
                            div(class=`${item_good}__name`)= item.name
                            img(src=`assets/img/content/cat-list.jpg` alt=`#`)

                +goods([
                    {
                        'img': 'cat-list.jpg',
                        'name': 'На бороны',
